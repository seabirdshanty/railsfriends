<h2> Active Users </h2>

<table class="table table-striped table-bordered table-hover">
	<thead class="table-dark">
		<th colspan=2>Name</th>
		<th>Email</th>
		<th></th>
	</thead>
	
<% @users.each do |user| %>
	<tr>
		<td><%= user.first_name %></td>
		<td><%= user.last_name %></td>
		<td><%= user.email %></td>
		<td> <%= link_to "Show", user %></td>
	</tr>
<% end %>
</table>
